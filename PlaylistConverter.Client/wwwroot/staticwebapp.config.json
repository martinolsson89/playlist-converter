{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_framework/*",
      "/css/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.ico",
      "/api/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "anonymous" ]
    }
  ],
  "mimeTypes": {
    ".css": "text/css",
    ".wasm": "application/wasm"
  }
}